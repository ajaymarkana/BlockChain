  "source": "//SPDX-License-Identifier: MIT\npragma solidity >= 0.5.2;\n\n\ncontract Messenger {\n\n  mapping (address => string) message;\n\n  function sendMessage(address _recipient, string memory _message) public{\n    message[_recipient] = _message;\n  }\n\n  function readMessage() view public returns (string memory){\n    return message[msg.sender];\n  }\n\n}\n\ncontract reader{\n    mapping (address => string) data;\n    function readData(address myaddress) public returns(string memory){\n    }\n}",
